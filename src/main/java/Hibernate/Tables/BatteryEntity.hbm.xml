<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Hibernate.Tables.BatteryEntity" table="Battery" schema="dbo" catalog="PolessBattery">
        <id name="batteryId" column="BatteryID"/>
        <property name="mark" column="Mark"/>
        <property name="capacity" column="Capacity"/>
        <property name="amperage" column="Amperage"/>
    </class>
</hibernate-mapping>