<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Hibernate.Tables.BrigadeEntity" table="Brigade" schema="dbo" catalog="PolessBattery">
        <id name="brigadeId">
            <column name="BrigadeID" sql-type="varchar(10)" length="10"/>
        </id>
        <property name="quantityEmployee">
            <column name="QuantityEmployee" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>