<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Hibernate.Tables.ComponentEntity" table="Component" schema="dbo" catalog="PolessBattery">
        <id name="componentId">
            <column name="ComponentID" sql-type="int"/>
        </id>
        <property name="nameComponent">
            <column name="NameComponent" sql-type="varchar(30)" length="30"/>
        </property>
    </class>
</hibernate-mapping>