<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Hibernate.Tables.CustomersEntity" table="Customers" schema="dbo" catalog="PolessBattery">
        <id name="customerId" column="CustomerID"/>
        <property name="organizationName" column="OrganizationName"/>
        <property name="address" column="Address"/>
        <property name="contactPerson" column="ContactPerson"/>
        <property name="phone" column="Phone"/>
        <property name="eMail" column="E-mail"/>
    </class>
</hibernate-mapping>