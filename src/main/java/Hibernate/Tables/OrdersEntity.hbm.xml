<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Hibernate.Tables.OrdersEntity" table="Orders" schema="dbo" catalog="PolessBattery">
        <id name="orderId" column="OrderID"/>
        <property name="quantityProduct" column="QuantityProduct"/>
        <property name="dateExecution" column="DateExecution"/>
    </class>
</hibernate-mapping>