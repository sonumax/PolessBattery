<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Hibernate.Tables.OrdersEntity" table="Orders" schema="dbo" catalog="PolessBattery">
        <id name="orderId">
            <column name="OrderID" sql-type="int"/>
        </id>
        <property name="quantityProduct">
            <column name="QuantityProduct" sql-type="float" precision="-1"/>
        </property>
        <property name="dateExecution">
            <column name="DateExecution" sql-type="date" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>