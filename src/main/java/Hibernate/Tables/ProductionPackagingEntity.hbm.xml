<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Hibernate.Tables.ProductionPackagingEntity" table="ProductionPackaging" schema="dbo"
           catalog="PolessBattery">
        <id name="planPackagingId">
            <column name="PlanPackagingID" sql-type="int"/>
        </id>
        <property name="quantityFinishProduct">
            <column name="QuantityFinishProduct" sql-type="float" precision="-1"/>
        </property>
        <property name="dateAssembly">
            <column name="DateAssembly" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>