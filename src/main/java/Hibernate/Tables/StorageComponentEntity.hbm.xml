<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Hibernate.Tables.StorageComponentEntity" table="StorageComponent" schema="dbo"
           catalog="PolessBattery">
        <id name="componentId">
            <column name="ComponentID" sql-type="int"/>
        </id>
        <property name="quantityProduct">
            <column name="QuantityProduct" sql-type="float" precision="-1"/>
        </property>
        <property name="unit">
            <column name="Unit" sql-type="varchar(10)" length="10"/>
        </property>
    </class>
</hibernate-mapping>